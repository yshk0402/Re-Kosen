#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/direction@1.0.4/node_modules/direction/node_modules:/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/direction@1.0.4/node_modules:/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/direction@1.0.4/node_modules/direction/node_modules:/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/direction@1.0.4/node_modules:/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../direction@1.0.4/node_modules/direction/cli.js" "$@"
else
  exec node  "$basedir/../../../../../direction@1.0.4/node_modules/direction/cli.js" "$@"
fi
