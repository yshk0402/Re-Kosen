#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/@strapi+strapi@5.33.3_@babel+runtime@7.28.6_@codemirror+autocomplete@6.20.0_@codemirror_c346cd42ed1445575354e39411dd0c90/node_modules/@strapi/strapi/bin/node_modules:/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/@strapi+strapi@5.33.3_@babel+runtime@7.28.6_@codemirror+autocomplete@6.20.0_@codemirror_c346cd42ed1445575354e39411dd0c90/node_modules/@strapi/strapi/node_modules:/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/@strapi+strapi@5.33.3_@babel+runtime@7.28.6_@codemirror+autocomplete@6.20.0_@codemirror_c346cd42ed1445575354e39411dd0c90/node_modules/@strapi/node_modules:/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/@strapi+strapi@5.33.3_@babel+runtime@7.28.6_@codemirror+autocomplete@6.20.0_@codemirror_c346cd42ed1445575354e39411dd0c90/node_modules:/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/@strapi+strapi@5.33.3_@babel+runtime@7.28.6_@codemirror+autocomplete@6.20.0_@codemirror_c346cd42ed1445575354e39411dd0c90/node_modules/@strapi/strapi/bin/node_modules:/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/@strapi+strapi@5.33.3_@babel+runtime@7.28.6_@codemirror+autocomplete@6.20.0_@codemirror_c346cd42ed1445575354e39411dd0c90/node_modules/@strapi/strapi/node_modules:/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/@strapi+strapi@5.33.3_@babel+runtime@7.28.6_@codemirror+autocomplete@6.20.0_@codemirror_c346cd42ed1445575354e39411dd0c90/node_modules/@strapi/node_modules:/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/@strapi+strapi@5.33.3_@babel+runtime@7.28.6_@codemirror+autocomplete@6.20.0_@codemirror_c346cd42ed1445575354e39411dd0c90/node_modules:/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@strapi+strapi@5.33.3_@babel+runtime@7.28.6_@codemirror+autocomplete@6.20.0_@codemirror_c346cd42ed1445575354e39411dd0c90/node_modules/@strapi/strapi/bin/strapi.js" "$@"
else
  exec node  "$basedir/../../../../../../@strapi+strapi@5.33.3_@babel+runtime@7.28.6_@codemirror+autocomplete@6.20.0_@codemirror_c346cd42ed1445575354e39411dd0c90/node_modules/@strapi/strapi/bin/strapi.js" "$@"
fi
