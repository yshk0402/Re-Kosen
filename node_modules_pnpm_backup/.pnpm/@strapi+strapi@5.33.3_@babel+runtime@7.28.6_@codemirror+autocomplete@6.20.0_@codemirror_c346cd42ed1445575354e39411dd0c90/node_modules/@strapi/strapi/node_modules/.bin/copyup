#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/copyfiles@2.4.1/node_modules/copyfiles/node_modules:/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/copyfiles@2.4.1/node_modules:/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/copyfiles@2.4.1/node_modules/copyfiles/node_modules:/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/copyfiles@2.4.1/node_modules:/home/yshk0402/デスクトップ/playground/Re-Kosen/cms/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../copyfiles@2.4.1/node_modules/copyfiles/copyfiles" "$@"
else
  exec node  "$basedir/../../../../../../copyfiles@2.4.1/node_modules/copyfiles/copyfiles" "$@"
fi
